{
  "Connection": {
    "Path": "/tmp/fpm-{{rand 1024}}.sock"
  },
  "Client": {
    "Connection": "default",
    "Script": "script.php"
  },
  "Server": {
    "Executable": "/usr/local/sbin/php-fpmi",
    "ConfigFile": "php-fpm.tmpl"
  },
  "Actions": [
    "server-start",
    {
      "Type": "client-request",
      "Client": "default",
      "Expect": {
        "Response": {
          "Body": "Done"
        },
        "Output": {
          "Regexp": "/error/"
        }
      }
    }
  ]
}
